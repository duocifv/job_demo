{
  "packageManager": "pnpm@8.7.1",
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.x <23.x"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
